            "cell_type": "code",
            "source": [
                "USE TREINO;\r\n",
                "\r\n",
                "SELECT a.NOME_CLIENTE, b.NOME_CIDADE, b.UF \r\n",
                "from CLIENTE AS a\r\n",
                "INNER JOIN CIDADE AS b\r\n",
                "on a.ID_CIDADE=b.ID_CIDADE;\r\n",
