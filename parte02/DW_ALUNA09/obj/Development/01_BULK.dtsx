    <DTS:Executable
      DTS:refId="Package\CARGA FATO"
      DTS:CreationName="STOCK:SEQUENCE"
      DTS:Description="Contêiner da Sequência"
      DTS:DTSID="{41B73C31-9577-485A-9219-C2C711F0AD5C}"
